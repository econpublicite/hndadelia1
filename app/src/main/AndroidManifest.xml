<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.econpublicite.hndadelia">
    <uses-permission android:name="android.permission.INTERNET" />



    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="HND ADELIA"
        android:usesCleartextTraffic="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar"
        >
        <activity
            android:name=".SplashcreemActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".FraganciaActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".MaquillajesActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".BienestarActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".CuerpoActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".CuidadoActivity"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>